{
  "scripts": {
    "bun-dev": "bun --watch ./src/server.ts",
    "dev": "nodemon --watch ./src/server.ts --exec \"ts-node\" -r tsconfig-paths/register ./src/server.ts",
    "generate:migration": "drizzle-kit generate:pg --out src/db/migrations --schema src/models/schema.ts",
    "migrate": "ts-node -r tsconfig-paths/register src/db/migrate"
  },
  "dependencies": {
    "@trpc/server": "^10.38.5",
    "@types/module-alias": "^2.0.2",
    "@types/node": "^20.8.2",
    "@types/pg": "^8.10.3",
    "@typescript-eslint/parser": "^6.7.4",
    "babel-eslint": "^10.1.0",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.19.13",
    "drizzle-orm": "^0.28.6",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-prettier": "^5.0.0",
    "module-alias": "^2.2.3",
    "nodemon": "^3.0.1",
    "path": "^0.12.7",
    "pg": "^8.11.3",
    "postgres": "^3.3.5",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "eslint-config-standard-with-typescript": "^39.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "_moduleAliases": {
    "@/*": "./src/*",
    "@utils/*": "./src/utils/*",
    "@models/*": "./src/models/*",
    "@controllers/*": "./src/controllers/*",
    "@services/*": "./src/services/*",
    "@config/*": "./src/config/*",
    "@routes/*": "./src/routes/*"
  },
  "_moduleDirectories": [
    "node_modules_custom"
  ]
}
